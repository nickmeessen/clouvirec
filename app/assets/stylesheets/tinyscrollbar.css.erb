#vpScroll { width: 100%; height: 100%; clear: both; }
#vpScroll .viewport { width: 95%; height: 100%; overflow: hidden; position: relative; }
#vpScroll .overview { list-style: none; position: absolute; left: 0; top: 0; }
#vpScroll .thumb .end,
#vpScroll .thumb { background-color: #003D5D; }
#vpScroll .scrollbar { position: relative; float: right; width: 15px; }
#vpScroll .track { background-color: #D8EEFD; height: 100%; width:13px; position: relative; padding: 0 1px; }
#vpScroll .thumb { height: 20px; width: 13px; cursor: pointer; overflow: hidden; position: absolute; top: 0; }
#vpScroll .thumb .end { overflow: hidden; height: 5px; width: 13px; }
#vpScroll .disable{ display: none; }
.noSelect { user-select: none; -o-user-select: none; -moz-user-select: none; -khtml-user-select: none; -webkit-user-select: none; }

